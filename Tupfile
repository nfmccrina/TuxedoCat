include_rules

cpp_files=$(SRCDIR)/LookupData.cpp $(SRCDIR)/Bitboard.cpp $(SRCDIR)/Square.cpp $(SRCDIR)/Main.cpp $(TESTDIR)/*.cpp

: foreach $(cpp_files) |> $(CC) $(CFLAGS) $(INCLUDES) -c %f -o %o |> $(BUILDDIR)/%B.o

: $(BUILDDIR)/*.o lib/libgtest.a |> $(CC) %f -pthread -o %o |> $(BUILDDIR)/tuxedocat
